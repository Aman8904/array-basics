public class productofele {  
    public static void main(String[] args) {  
        int [] arr = new int [] {1, 2, 3, 4, 5};  
        int pro = 0;
        for (int i = 0; i < arr.length; i++) {  
           pro = pro + arr[i];  
        }  
        System.out.println("product of all the elements of an array: " + pro);  
    }  
}  
